{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "$id": "profile.json",
    "type": "object",
    "title": "Profile",
    "description": "A user profile containing members and invitations",
    "deprecated": false,
    "properties": {
        "id": {
            "type": "string",
            "format": "uuid",
            "description": "Unique identifier for the profile",
            "examples": [
                "987e6543-e21b-34c3-b456-426614174999",
                "abc12345-f678-90de-f123-456789abcdef"
            ]
        },
        "members": {
            "type": "array",
            "description": "Array of profile members",
            "items": {
                "type": "object",
                "description": "Array of profile members",
                "$ref": "profile_member.json"
            }
        }
    },
    "required": [
        "id",
        "members"
    ],
    "additionalProperties": false
}